{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": ["es2017"],
    "allowJs": true,
    "jsx": "react-native",
    "noEmit": true,
    "isolatedModules": true,
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "./src/",
    "paths": {
      "assets/*": ["assets/*"],
      "common/*": ["common/*"],
      "components/*": ["components/*"],
      "hooks/*": ["hooks/*"],
      "mocks/*": ["mocks/*"],
      "navigation/*": ["navigation/*"],
      "screens/*": ["screens/*"],
      "store/*": ["store/*"],
      "utils/*": ["utils/*"]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "react-native.config.js"
  ]
}
